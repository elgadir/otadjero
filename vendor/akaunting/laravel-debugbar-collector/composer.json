{
    "name": "akaunting/laravel-debugbar-collector",
    "description": "Laravel Debugbar collector package for Otadjer",
    "keywords": [
        "otadjer",
        "laravel",
        "debugbar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis DuliÃ§i",
            "email": "info@otadjer.com",
            "homepage": "https://otadjer.com",
            "role": "Developer"
        },
        {
            "name": "Sevan Nerse",
            "email": "sevan@otadjer.com",
            "homepage": "https://otadjer.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "barryvdh/laravel-debugbar": "^3.6"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\DebugbarCollector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akaunting\\DebugbarCollector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\DebugbarCollector\\Provider"
            ]
        }
    }
}
